} from 'react';\nimport { BrowserRouter as Router, Route } from 'react-router-dom';\n// state\nimport { useDispatch, useSelector } from 'react-redux';\nimport { fetchUser } from './actions/authActions';\n// components\nimport Header from './components/navigation/Header';\nimport Landing from './components/pages/landing/Landing';\nimport Profile from './components/pages/profile/Profile';\nimport Surveys from './components/pages/surveys/Surveys';\nimport SurveyForm from './components/pages/surveyForm/SurveyForm';\nimport SurveyDetail from './components/pages/surveys/SurveyDetail';\nimport PrivateRoute from './components/navigation/PrivateRoute';\n\nconst App = () => {\n  const dispatch = useDispatch();\n  const auth = useSelector((state) => state.auth);\n  // get logged in user\n  useEffect(() => {\n    dispatch(fetchUser());\n  }, []);\n\n  return (\n    <div>\n      <Router>\n        <Header />\n        <Route exact path='/' component={Landing} />\n        <PrivateRoute exact path='/profile' component={Profile} />\n        <PrivateRoute exact path='/surveys' component={Surveys} />\n        <PrivateRoute path='/surveys/:id' component={SurveyDetail} />\n        <PrivateRoute exact path='/surveys/new' component={SurveyForm} />\n      </Router>\n    </div>\n  );\n};\n\nexport default App;\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/reducers/index.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/reducers/authReducer.js",["109"],"import { FETCH_USER } from '../actions/types';\n\nconst initialState = {\n  credits: 0,\n  _id: null,\n  googleId: null,\n  displayName: null,\n  emailAddress: null,\n};\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case FETCH_USER:\n      return action.payload || false;\n    default:\n      return state;\n  }\n};\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/actions/authActions.js",[],"/Users/michaelmunn/WebDev/R