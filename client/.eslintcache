y' aria-label='add'>\n          <Fab\n            component={Link}\n            to='/surveys/new'\n            color='secondary'\n            className={classes.fab}\n          >\n            <AddIcon />\n          </Fab>\n        </Tooltip>\n      </div>\n    </Container>\n  );\n};\n\nexport default Surveys;\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveyForm/SurveyForm.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveyForm/Confirmation.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveyForm/SurveyFormSteps.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/utils/validateEmails.js",["111","112","113"],"// from http://emailregex.com/\nconst re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n\nexport default (emails) => {\n  const invalidEmails = emails\n    .split(',')\n    .map((email) => email.trim())\n    .filter((email) => re.test(email) === false);\n\n  if (invalidEmails.length) {\n    return `These emails are invalid: ${invalidEmails}`;\n  }\n};\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/utils/validateForm.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveyForm/formFields.js",["114"],"// to edit form fields, change in this file\nexport default [\n  { id: 'title', label: 'Survey Title', multiline: false, rows: 0 },\n  { id: 'subject', label: 'Subject Line', multiline: false, rows: 0 },\n  { id: 'body', label: 'Email Body', multiline: true, rows: 5 },\n  { id: 'recipients', label: 'Recipient List', multiline: true, rows: 1 },\n];\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/reducers/surveyReducer.js",["115"],"import {\n  CREATE_SURVEY,\n  FETCH_SURVEYS,\n  FETCH_CURRENT_SURVEY,\n  SURVEY_LOADING,\n  SURVEY_ERROR,\n} from '../actions/types';\n\nconst initialState = {\n  surveys: [],\n  current: {\n    yes: 0,\n    no: 0,\n    title: null,\n    subject: null,\n    body: null,\n    recipients: [],\n  },\n  loading: false,\n  error: null,\n};\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case SURVEY_LOADING:\n      return {\n        ...state,\n        loading: true,\n      };\n    case SURVEY_ERROR:\n      return {\n        ...state,\n        error: action.payload,\n        loading: false,\n      };\n    case CREATE_SURVEY:\n      return {\n        ...state,\n        surveys: [...state.surveys, action.payload],\n        loading: false,\n      };\n    case FETCH_SURVEYS:\n      return {\n        ...state,\n        surveys: action.payload,\n        loading: false,\n      };\n    case FETCH_CURRENT_SURVEY:\n      return {\n        ...state,\n        current: action.payload,\n        loading: false,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/actions/surveyActions.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveys/SurveyList.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/navigation/PrivateRoute.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveys/SurveyListItem.js",[],"/Users/michaelmunn/WebDev/React/node-with-react-grider/server/client/src/components/pages/surveys/SurveyDetail.js",["116","117","118","119","120","121","122","123"],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":17,"column":9,"nodeType":"130","messageId":"131","endLine":17,"endColumn":13},{"ruleId":"132","severity":1,"message":"133","line":21,"column":6,"nodeType":"134","endLine":21,"en